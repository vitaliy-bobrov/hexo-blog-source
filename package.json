{
  "name": "metalsthith-blog-source",
  "version": "1.0.0",
  "description": "My blog using metalsmith static generator.",
  "main": "metalsmith.js",
  "private": true,
  "scripts": {
    "start": "gulp && gulp serve",
    "run:prod": "yarn build:prod && gulp serve",
    "build": "gulp",
    "build:prod": "cross-env NODE_ENV=production gulp",
    "deploy": "yarn build:prod && cross-env NODE_ENV=production gulp deploy",
    "seo": "gulp seo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaliy-bobrov/metalsmith-blog-source.git"
  },
  "author": "Vitaliy Bobrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitaliy-bobrov/metalsmith-blog-source/issues"
  },
  "homepage": "https://github.com/vitaliy-bobrov/metalsmith-blog-source#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "async-once": "^1.0.1",
    "autoprefixer": "^9.3.1",
    "browser-sync": "^2.24.7",
    "cross-env": "^5.2.0",
    "css-mqpacker": "^7.0.0",
    "date-fns": "^1.29.0",
    "del": "^3.0.0",
    "escape-html": "^1.0.3",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-babel-minify": "^0.5.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-uncss": "^1.0.6",
    "gulp-webp": "^3.0.0",
    "handlebars": "^4.0.6",
    "metalsmith": "^2.3.0",
    "metalsmith-author": "^1.1.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-debug": "^1.2.0",
    "metalsmith-default-values": "^3.0.0",
    "metalsmith-discover-partials": "^0.1.2",
    "metalsmith-disqus": "^1.2.4",
    "metalsmith-drafts": "^0.1.0",
    "metalsmith-excerpts": "^1.2.0",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-mapsite": "^1.0.6",
    "metalsmith-markdown-remarkable": "^0.1.2",
    "metalsmith-pagination": "^1.4.1",
    "metalsmith-permalinks": "^2.0.0",
    "metalsmith-prism": "^3.1.1",
    "metalsmith-register-helpers": "^0.4.0",
    "metalsmith-tags": "^2.0.0",
    "metalsmith-twitter-card": "^1.0.4",
    "metalsmith-updated": "^0.1.6",
    "postcss-assets": "^5.0.0",
    "postcss-mq-keyframes": "^0.3.0",
    "remarkable-classy": "^0.0.1",
    "remarkable-emoji": "^0.1.3",
    "remarkable-extlink": "^1.1.0",
    "remarkable-youtube": "^1.2.1",
    "workbox-build": "^3.6.3"
  },
  "dependencies": {
    "material-design-lite": "^1.3.0",
    "mdl-ext": "^0.17.1"
  },
  "browserslist": [
    "last 3 chrome versions",
    "last 3 and_chr versions",
    "android >= 6",
    "last 3 firefox versions",
    "last 3 edge versions",
    "safari >= 11",
    "iOS >= 11"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "info",
            "warn",
            "error"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    },
    "globals": {
      "ga": false,
      "registerPaint": false,
      "componentHandler": false
    }
  }
}
