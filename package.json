{
  "name": "metalsthith-blog-source",
  "version": "1.0.0",
  "description": "My blog using metalsmith static generator.",
  "main": "metalsmith.js",
  "private": true,
  "scripts": {
    "start": "gulp && gulp serve:dev",
    "build": "gulp",
    "build:prod": "cross-env NODE_ENV=production gulp",
    "deploy": "yarn build:prod && cross-env NODE_ENV=production gulp deploy",
    "seo": "gulp seo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaliy-bobrov/metalsmith-blog-source.git"
  },
  "author": "Vitaliy Bobrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitaliy-bobrov/metalsmith-blog-source/issues"
  },
  "homepage": "https://github.com/vitaliy-bobrov/metalsmith-blog-source#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "autoprefixer": "^8.6.0",
    "browser-sync": "^2.18.13",
    "cross-env": "^5.1.5",
    "css-mqpacker": "^6.0.1",
    "date-fns": "^1.29.0",
    "del": "^3.0.0",
    "escape-html": "^1.0.3",
    "eslint-config-google": "^0.9.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-babel-minify": "^0.4.3",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^4.0.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.4.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-svg-sprite": "^1.3.6",
    "gulp-uncss": "^1.0.6",
    "gulp-webp": "^3.0.0",
    "handlebars": "^4.0.6",
    "metalsmith": "^2.3.0",
    "metalsmith-author": "^1.1.0",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-debug": "^1.1.0",
    "metalsmith-default-values": "^2.0.0",
    "metalsmith-discover-partials": "^0.1.0",
    "metalsmith-disqus": "^1.2.2",
    "metalsmith-drafts": "^0.0.1",
    "metalsmith-excerpts": "^1.1.0",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-mapsite": "^1.0.4",
    "metalsmith-markdown-remarkable": "^0.1.1",
    "metalsmith-pagination": "^1.4.0",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-prism": "^3.1.1",
    "metalsmith-register-helpers": "^0.4.0",
    "metalsmith-tags": "^2.0.0",
    "metalsmith-twitter-card": "^1.0.3",
    "metalsmith-updated": "^0.1.4",
    "postcss-assets": "^5.0.0",
    "postcss-mq-keyframes": "^0.3.0",
    "remarkable-classy": "^0.0.1",
    "remarkable-emoji": "^0.1.3",
    "remarkable-extlink": "^1.1.0",
    "remarkable-youtube": "^1.0.2",
    "run-sequence": "^2.1.0",
    "sw-offline-google-analytics": "^0.0.25",
    "sw-precache": "^5.2.0",
    "sw-toolbox": "^3.4.0"
  },
  "dependencies": {
    "material-design-lite": "^1.3.0",
    "mdl-ext": "^0.17.0"
  },
  "browserslist": [
    "last 3 chrome versions",
    "last 3 and_chr versions",
    "last 3 and_ff versions",
    "last 3 and_uc versions",
    "android >= 6",
    "last 3 firefox versions",
    "last 2 samsung versions",
    "last 3 edge versions",
    "last 2 opera versions",
    "safari >= 10",
    "iOS >= 10",
    "ie >= 11"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-console": [
        "warn",
        {
          "allow": [
            "info",
            "warn",
            "error"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    },
    "globals": {
      "ga": false,
      "registerPaint": false,
      "componentHandler": false
    }
  }
}
